                            "cn": [
                                {
                                    "type": "viewcontrollereventbinding",
                                    "reference": {
                                        "name": "listeners",
                                        "type": "array"
                                    },
                                    "codeClass": null,
                                    "userConfig": {
                                        "fn": "onBtnLugarGuardarClick",
                                        "implHandler": [
                                            "var form = Ext.getCmp('frmNuevoLugar').getForm(),",
                                            "    store = Ext.getStore('Lugar');",
                                            "if (form.isValid()) {",
                                            "    Ext.Ajax.request({",
                                            "        url: '../servidor/reservas_puce/lugar/verificar',",
                                            "        method:\"post\",",
                                            "        params: {",
                                            "            descripcion: Ext.getCmp('txtNuevoLugarDescripcion').getValue()",
                                            "        },",
                                            "        success: function( result, request ) {",
                                            "            var responseData = Ext.JSON.decode(result.responseText);",
                                            "            if(responseData.success)",
                                            "            {",
                                            "",
                                            "                var values = form.getValues();",
                                            "",
                                            "                store.add(values);",
                                            "                store.proxy.extraParams = {",
                                            "                    id_lugar: Ext.getCmp('cbNuevoLugarLugar').getValue()",
                                            "                };",
                                            "                store.sync(",
                                            "                    {",
                                            "                        success: function(batch, options)",
                                            "                        {",
                                            "                            Ext.getCmp('wndNuevoLugar').close();",
                                            "                            store.load();",
                                            "                            Ext.Msg.alert('Datos Guardados','Se han guardado los datos exitosamente');",
                                            "                        },",
                                            "                        failure: function(batch, options)",
                                            "                        {",
                                            "",
                                            "                            Ext.Msg.alert('Advertencia', 'Hubo un error.');",
                                            "                            store.load();",
                                            "                        }",
                                            "                    }",
                                            "                );",
                                            "            }",
                                            "            else",
                                            "            {",
                                            "                Ext.Msg.alert('Alerta','Este lugar ya está ingresado.');",
                                            "            }",
                                            "        },",
                                            "",
                                            "        failure: function(response, opts) {",
                                            "            Ext.Msg.alert('Alerta','Este lugar ya está ingresado.');",
                                            "",
                                            "        }",
                                            "    });",
                                            "}",
                                            "else",
                                            "{",
                                            "    Ext.Msg.alert('Error','Ingrese los datos correctamente');",
                                            "}",
                                            ""
                                        ],
                                        "name": "click",
                                        "scope": "me"
                                    },
